{
    "manifest_version": 2,
    "name": "Nepali Date Extension",
    "version": "1.0",
    "description": "Displays today's Nepali full date.",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }
  